QFrame{
    background-color: #FEFEFE;
    border: 0px solid gray;
    color: #444444;
}

/*====================== window ======================*/
QFrame#window_title_frame{
    background-color: rgba(31, 41, 46, 255);
    color: #FFFFFF;
}

QLabel#window_menu_label{
    height: 32;
    font-size: 15px;
    color: #A5A5A5;
    background-color: rgba(0,0,0,0);
    border: 0px solid;
    Text-align: left;
    padding-left: 10;
    margin: 0px;
}

/*====================== frame ======================*/
QFrame#link_frame{
    background-color: #FFFFFF;
}

QFrame#head_frame{
    background-color: #F1F1F1;
}

QFrame#attr_frame{
    background-color: rgba(241, 241, 247, 255);
}

QFrame#operation_frame{
    background-color: #FAFAFA;
}

QScrollArea{
    background-color: transparent
}

/*====================== checkbox ======================*/
QCheckBox{
    
    font: 13px;
    color: #444444;
}
QCheckBox:unchecked{ color: #999999; }

/*====================== combobox ======================*/
QComboBox {
    font: bold 12px;
    color: #442342;
    border: 1px solid gray;
    border-radius: 0px;
    padding: 1px 2px 1px 2px;
    min-width: 9em;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
 
    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

QTextEdit{
    
    font: bold 12px;
    background-color: rgba(220, 231, 232, 255);
}

/* tool button */
QToolButton{
    
    font: bold 12px;
    qproperty-iconSize: 20px;
    color: rgba(68, 35, 66, 255);
    Text-align: center;
}

QToolButton { /* all types of tool button */
    border: 0px solid #8f8f91;
    border-radius: 2px;
    background-color:#fecf39;
    color: #444444;
    padding-left: 10px;
    padding-right: 10px;
    /*background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #f6f7fa, stop: 1 #dadbde);
    */
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 20px; /* make way for the popup button */
}

QToolButton:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #dadbde, stop: 1 #f6f7fa);
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 0px solid gray;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
    padding-left: 20px;
}

QToolButton::menu-arrow:open {
    top: 1px; left: 1px; /* shift it a bit */
}

/*====================== QPushButton ======================*/
QPushButton{
    font: 14px;
    color: #442342;
    border: 0px solid gray;
    background-color: transparent;
}

QPushButton:menu-indicator{
    width:0px;
}

QPushButton#value_button{
    background-color: transparent;
    border: 1px solid #C2C7CB;
}

QPushButton#link_button{
    font-family: Arial;
    background-color: transparent;
    color: #626262;
    font: 14px;
    qproperty-iconSize: 24px;
}

QPushButton#refresh_button{
    qproperty-iconSize: 18px;   
}

QPushButton#new_button{
    color: #FFFFFF;
    background-color: #3BAFDA;
    Text-align: center;
}
QPushButton#new_button:hover{
    color: #444444;
    background-color: #4FC1E9;
}
QPushButton#new_button:pressed{
    color: #000000;
    background-color: #3BAFDA;
}

QPushButton#create_button{
    Text-align: center;
    background-color: rgba(66, 209, 127, 255);
    color: #FFFFFF;
    qproperty-iconSize: 16px;
}

QPushButton#create_button:hover{
    color: #FFFFFF;
    background-color: rgba(66, 189, 107, 255);
}

QPushButton#create_button:pressed{
    color: #FFFFFF;
    background-color: #4FC1E9;
    border-bottom: 0px solid #444444;
}

QPushButton#create_button:pressed{
    color: #FFFFFF;
    background-color: #4FC1E9;
    border-bottom: 0px solid #444444;
}

QPushButton#create_button:!enabled{
    color: #AAAAAA;
    background-color: #444444;
}

QPushButton#cancel_button{
    Text-align: center;
    background-color: rgba(255, 148, 147, 255);
    color: #FFFFFF;
    qproperty-iconSize: 16px;
}

QPushButton#cancel_button:hover{
    color: #FFFFFF;
    background-color: rgba(255, 128, 127, 255);
}

QPushButton#cancel_button:pressed{
    background-color: #FF0000;
    color: #FFFFFF;
    border-bottom: 0px solid #444444;
}

/*====================== support button ======================*/
QPushButton#support_button{
    font: bold 13px;
    background-color: #E6E7EA;
