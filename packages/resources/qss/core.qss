/*====================== QListView ======================*/
QListView{
    border-top: 1px solid #F5F5F7;
    background-color: #EBEDEF;
}



/*====================== QScrollBar ======================*/
QScrollBar:vertical{
    width:10px;
    background:rgba(240, 240, 240, 255);
    margin:0px,0px,0px,0px;
    padding-top:0px;
    padding-bottom:0px;}
QScrollBar::handle:vertical{
    width:10px;
    background:#A6A6A6;
    border-radius:4px;
    min-height:20;}
QScrollBar::handle:vertical:hover{
    width:10px;
    background:#A6A6A6;
    border-radius:4px;
    min-height:20;}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical{
    background:rgba(240, 240, 240, 255);
    border-radius:0px;}
QScrollBar:horizontal{
    height:10px;
    background:rgba(240, 240, 240, 255);
    margin:0px,0,0px,0;
    padding-left:0px;
    padding-right:0px;}
QScrollBar::handle:horizontal{
    height:10px;
    background:#A6A6A6;
    border-radius:4px;
    min-width:20px;}
QScrollBar::handle:horizontal:hover{
    height:10px;
    background:#A6A6A6;
    border-radius:4px;
    min-width:20;}
QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal{
    background:rgba(240, 240, 240, 255);
    border-radius:0px;}

